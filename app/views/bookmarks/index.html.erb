<div class="row mt-5 pt-5 pr-n3 ">
  <div class="col-9">
    <% if flash[:notice] %>
      <p class="notice" > <%= flash[:notice] %> </p>
    <% end %>
    <% if flash[:alert] %>
      <p class="alert"> <%= flash[:alert] %> </p>
    <% end %>
    <%= render 'layouts/header' %>
    <h5>List of Bookmarks</h5>
    <%= render 'bookmarks' %>
  </div>
  <div class="col-3">
    <ul class="trends list-group pr-0 pt-3">
      <li class="pt-3 pb-3 list-group-item text-uppercase">Trends for you</li>
      <%= render 'trends' %>
    </ul>
  </div>
</div>
